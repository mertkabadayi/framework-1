{
    "name": "pagekit/framework",
    "description": "The Pagekit Framework",
    "keywords": ["framework", "pagekit"],
    "homepage": "http://pagekit.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Pagekit Team",
            "email": "info@pagekit.com"
        }
    ],
    "require": {
        "doctrine/annotations": "1.*, >=1.2.1",
        "doctrine/dbal": "2.5.*",
        "ircmaxell/password-compat": "1.0.*",
        "ircmaxell/random-lib": "1.0.*",
        "symfony/console": "2.6.*",
        "symfony/finder": "2.6.*",
        "symfony/http-foundation": "2.6.*",
        "symfony/http-kernel": "2.6.*",
        "symfony/routing": "2.6.*",
        "symfony/stopwatch": "2.6.*",
        "symfony/templating": "2.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Pagekit\\": "application/src",
            "Pagekit\\Auth\\": "auth/src",
            "Pagekit\\Config\\": "config/src",
            "Pagekit\\Cookie\\": "cookie/src",
            "Pagekit\\Database\\": "database/src",
            "Pagekit\\Filesystem\\": "filesystem/src",
            "Pagekit\\Filter\\": "filter/src",
            "Pagekit\\Module\\": "module/src",
            "Pagekit\\Routing\\": "routing/src",
            "Pagekit\\Session\\": "session/src",
            "Pagekit\\View\\": "view/src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.9-dev"
        }
    }
}
